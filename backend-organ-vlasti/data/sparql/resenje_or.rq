SELECT * FROM <%s> WHERE {
  ?doc <%s> ?datum .
  ?doc <%s> ?tipZalbe .
  ?doc <%s> ?status .
  ?doc <%s> ?izdatoU .
  ?doc <%s> ?organVlasti .
  FILTER( CONTAINS(UCASE(str(?datum)), UCASE("%s"))
    || CONTAINS(UCASE(str(?tipZalbe)), UCASE("%s"))
    || CONTAINS(UCASE(str(?status)), UCASE("%s"))
    || CONTAINS(UCASE(str(?izdatoU)), UCASE("%s"))
    || CONTAINS(UCASE(str(?organVlasti)), UCASE("%s"))
  ) .
} 
