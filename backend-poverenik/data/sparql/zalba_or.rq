SELECT * FROM <%s> WHERE {
  ?doc <%s> ?datum .
  ?doc <%s> ?mesto .
  ?doc <%s> ?mestoIzdavanja .
  ?doc <%s> ?organVlasti .
  ?doc <%s> ?tip .
  ?doc <%s> ?status .
  FILTER( CONTAINS(UCASE(str(?datum)), UCASE("%s"))
    || CONTAINS(UCASE(str(?mesto)), UCASE("%s"))
    || CONTAINS(UCASE(str(?mestoIzdavanja)), UCASE("%s"))
    || CONTAINS(UCASE(str(?organVlasti)), UCASE("%s"))
    || CONTAINS(UCASE(str(?tip)), UCASE("%s"))
    || CONTAINS(UCASE(str(?status)), UCASE("%s"))
  ) .
} 
